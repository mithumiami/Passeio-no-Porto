<p id="notice"></p>
<div class="container">
  <div class="row">
    <div class="col-sm-12 mx-auto">


      <h4 class="my-3 ml-3">Posts</h4>
    </div>

        <% @posts.each do |post| %>
        <div class="col-md-3">
          <div class="product-item">
            <section class>
              <%= link_to admin_post_path(post.id) do %>
                <%= attachment_image_tag post, :image, :fill, 200, 150, format: 'jpeg', fallback: "noimage.png" %>
              <% end %>
              <div><%= post.title %></div>
            </section>
          </div>
        </div>
      <% end %>
      </div>
   </div>
</div>